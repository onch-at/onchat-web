{
  "name": "onchat",
  "version": "0.0.1",
  "author": "HyperLife1119",
  "description": "A simple, beautiful, mobile-first instant messaging web application build with Angular and Ionic.",
  "homepage": "https://chat.hypergo.net",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host=0.0.0.0",
    "https": "ng serve --host=0.0.0.0 --ssl true --ssl-cert localhost.pem --ssl-key localhost-key.pem",
    "build": "ionic build --prod --aot --build-optimizer --engine=browser",
    "bundle-report": "ng build --prod --aot --build-optimizer --stats-json && webpack-bundle-analyzer --port=9111 www/stats.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "ngcc",
    "clean-cache": "npm cache clean --force"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.12",
    "@angular/cdk": "^11.2.11",
    "@angular/common": "^11.2.12",
    "@angular/core": "^11.2.12",
    "@angular/forms": "^11.2.12",
    "@angular/platform-browser": "^11.2.12",
    "@angular/platform-browser-dynamic": "^11.2.12",
    "@angular/router": "^11.2.12",
    "@angular/service-worker": "^11.2.12",
    "@capacitor/android": "^2.4.7",
    "@capacitor/core": "2.4.7",
    "@ionic/angular": "^5.6.6",
    "core-js": "^3.11.1",
    "hammerjs": "^2.0.8",
    "ngx-image-cropper": "^3.3.5",
    "ngx-quill": "^13.4.0",
    "ngx-socket-io": "^3.3.1",
    "quill": "^1.3.7",
    "rxjs": "^6.6.7",
    "swiper": "^6.5.8",
    "tslib": "^2.0.0",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.11",
    "@angular-eslint/builder": "4.2.0",
    "@angular-eslint/eslint-plugin": "4.2.0",
    "@angular-eslint/eslint-plugin-template": "4.2.0",
    "@angular-eslint/schematics": "4.2.0",
    "@angular-eslint/template-parser": "4.2.0",
    "@angular/cli": "^11.2.11",
    "@angular/compiler": "^11.2.12",
    "@angular/compiler-cli": "^11.2.12",
    "@angular/language-service": "^11.2.12",
    "@capacitor/cli": "2.4.7",
    "@ionic/angular-toolkit": "^2.3.3",
    "@ionic/eslint-config": "^0.3.0",
    "@types/dom-mediacapture-record": "^1.0.7",
    "@types/jasmine": "^3.6.10",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^14.14.43",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "eslint": "^7.26.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^5.2.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "^7.0.0",
    "ts-node": "~9.0.0",
    "typescript": "~4.0.2",
    "webpack-bundle-analyzer": "^4.4.1"
  },
  "eslintConfig": {
    "extends": "@ionic/eslint-config/recommended"
  }
